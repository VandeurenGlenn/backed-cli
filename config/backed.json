{
  "bundles": [
    {
      "src": "src/backed-cli",
      "dest": "dist/backed-cli.js",
      "plugins": {
        "inject-template": {}
      },
      "external": [
        "lodash"
      ]
    }
  ],
  "format": "es",
  "sourceMap": true,
  "sources": {
    "src": ["*/templates/*"],
    "dest": "dist/templates"
  },
  "watch": [{
    "task": "build",
    "src": ["./src"],
    "options": {}
  }]
}
